<%= govuk_cookie_banner do |cb| %>
  <% cb.with_message(heading_text: "Cookies on Request personal information") do |m| %>
    <% m.with_action { govuk_button_link_to("Accept analytics cookies", "/cookies/accept") } %>
    <% m.with_action { govuk_button_link_to("Reject analytics cookies", "/cookies/reject") } %>
    <% m.with_action { govuk_link_to("View cookies", "/cookies") } %>

    <%= t '.content_html' %>
  <% end %>
<% end %>
